{
	"name": "xwp/wp-js-widgets",
	"description": "The next generation of widgets in Core (Widgets 3.0), embracing JS for UI and powering the Widgets REST API.",
	"version": "0.4.1",
	"type": "wordpress-plugin",
	"keywords": [ "customizer", "widgets", "rest-api" ],
	"homepage": "https://github.com/xwp/wp-js-widgets/",
	"license": [ "GPL-2.0+" ],
	"repositories": [
		{
			"type": "git",
			"url": "https://github.com/xwp/wp-js-widgets.git"
		}
	],
	"dist": {
		"url": "https://downloads.wordpress.org/plugin/js-widgets.zip",
		"type": "zip"
	},
	"require": {
		"php": ">=5.2.0"
	},
	"authors": [
		{
			"name": "Weston Ruter",
			"homepage": "https://weston.ruter.net/"
		}
	],
	"require-dev": {
		"behat/behat": "^3.3",
		"behat/mink-extension": "^2.2",
		"behat/mink-goutte-driver": "^1.2",
		"behat/mink-selenium2-driver": "^1.3",
		"satooshi/php-coveralls": "dev-master"
	},
	"autoload": {
		"psr-4": { "XWP\\WordPress\\Behat\\": "tests/behat/features/bootstrap" }
	}
}
